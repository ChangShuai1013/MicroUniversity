<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.4">
  <display-name>server</display-name>
  <description>server</description>
  <filter>
    <filter-name>CharacterEncodingFilter</filter-name>
    <filter-class>com.changshuai.util.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>CharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>OrderEditServlet</servlet-name>
    <servlet-class>com.changshuai.order.OrderEditServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>OrderServlet</servlet-name>
    <servlet-class>com.changshuai.order.OrderServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShopMgrServlet</servlet-name>
    <servlet-class>com.changshuai.shop.ShopMgrServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShopEditServlet</servlet-name>
    <servlet-class>com.changshuai.shop.ShopEditServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>com.changshuai.admin.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.changshuai.admin.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdatePasswordServlet</servlet-name>
    <servlet-class>com.changshuai.admin.UpdatePasswordServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.changshuai.user.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TypeMgrServlet</servlet-name>
    <servlet-class>com.changshuai.type.TypeMgrServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DataServlet</servlet-name>
    <servlet-class>com.changshuai.data.DataServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LiuyanbanAction</servlet-name>
    <servlet-class>com.changshuai.action.LiuyanbanAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LiuyanbanAction</servlet-name>
    <url-pattern>/liuyanban.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DataServlet</servlet-name>
    <url-pattern>/data.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TypeMgrServlet</servlet-name>
    <url-pattern>/type.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/user.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdatePasswordServlet</servlet-name>
    <url-pattern>/updatePassword.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OrderEditServlet</servlet-name>
    <url-pattern>/orderEdit.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OrderServlet</servlet-name>
    <url-pattern>/order.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShopEditServlet</servlet-name>
    <url-pattern>/shopEdit.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShopMgrServlet</servlet-name>
    <url-pattern>/shopList.do</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>